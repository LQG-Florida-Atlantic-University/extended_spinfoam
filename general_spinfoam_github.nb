         RowBox[{"[", "i", "]"}], "]"}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"#", "  ", "-", 
          RowBox[{
           RowBox[{"1", "/", "2"}], " ", 
           RowBox[{"IdentityMatrix", "[", "2", "]"}]}]}], "  ", "&"}], " ", "/@", 
        RowBox[{"(", " ", 
         RowBox[{
          RowBox[{"tetn0sign", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"Transpose", "[", 
               RowBox[{"{", "#", "}"}], "]"}], ".", 
              RowBox[{"Conjugate", "[", 
               RowBox[{"{", "#", "}"}], "]"}], ".", 
              RowBox[{"PauliMatrix", "[", "3", "]"}]}], " ", "&"}], " ", "/@",
             " ", 
            RowBox[{"bdyxi", "[", 
             RowBox[{"[", "i", "]"}], "]"}]}], ")"}]}], ")"}]}]}], "]"}], ",", 
