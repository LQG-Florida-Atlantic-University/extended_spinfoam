Cell[BoxData[
 RowBox[{
  RowBox[{"bdybivec55s", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"sgndet", "[", 
         RowBox[{"[", "i", "]"}], "]"}], ">", "0"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Transpose", "[", 
            RowBox[{"{", "#", "}"}], "]"}], ".", 
           RowBox[{"Conjugate", "[", 
            RowBox[{"{", "#", "}"}], "]"}]}], " ", "-", 
          RowBox[{
           RowBox[{"1", "/", "2"}], " ", 
           RowBox[{"IdentityMatrix", "[", "2", "]"}]}]}], " ", "&"}], " ", "/@",
         " ", 
        RowBox[{"bdyxi", "[", 
         RowBox[{"[", "i", "]"}], "]"}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"#", "  ", "-", 
          RowBox[{
           RowBox[{"1", "/", "2"}], " ", 
           RowBox[{"IdentityMatrix", "[", "2", "]"}]}]}], "  ", "&"}], " ", "/@", 
        RowBox[{"(", " ", 
         RowBox[{
          RowBox[{"tetn0sign", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"Transpose", "[", 
               RowBox[{"{", "#", "}"}], "]"}], ".", 
              RowBox[{"Conjugate", "[", 
               RowBox[{"{", "#", "}"}], "]"}], ".", 
              RowBox[{"PauliMatrix", "[", "3", "]"}]}], " ", "&"}], " ", "/@",
             " ", 
            RowBox[{"bdyxi", "[", 
             RowBox[{"[", "i", "]"}], "]"}]}], ")"}]}], ")"}]}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", 
       RowBox[{"Length", "[", "sgndet", "]"}]}], "}"}]}], "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.836564731270741*^9, 3.836564909890745*^9}, {
